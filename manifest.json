{
    "name": "AO3 Settings",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "base_icon.png",
        "32": "base_icon.png",
        "48": "base_icon.png",
        "128": "base_icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "base_icon.png",
            "32": "base_icon.png",
            "48": "base_icon.png",
            "128": "base_icon.png"
        }
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ]
}